{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:05:00",
  "httpWorker": {
    "description": {
      "defaultExecutablePath": "python",
      "defaultWorkerPath": "azure/functions_worker"
    }
  },
  "cors": {
    "supportCredentials": false,
    "allowedOrigins": [
      "https://tcdynamics.fr",
      "https://www.tcdynamics.fr",
      "http://localhost:8080",
      "http://localhost:3000"
    ]
  }
}

