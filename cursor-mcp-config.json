{
  "name": "TCDynamics MCP Auto-Start",
  "description": "Automatically start NIA MCP server when opening Cursor",
  "command": "cmd",
  "args": ["/c", "start-mcp-silent.bat"],
  "cwd": "${workspaceFolder}",
  "type": "shell",
  "presentation": {
    "echo": false,
    "reveal": "never",
    "focus": false,
    "panel": "shared",
    "showReuseMessage": false,
    "clear": true
  },
  "runOptions": {
    "runOn": "folderOpen"
  },
  "problemMatcher": []
}
